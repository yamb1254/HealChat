{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "builds": [
      {
        "src": "HCBackend/src/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "HealChat/vite.config.ts",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/HCBackend/(.*)",
        "dest": "/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/HealChat/$1"
      }
    ]
  }
  